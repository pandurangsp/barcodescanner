<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oraclecorp.internal.ent1.vbcs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YourAppName</name>
    <description>
        Your application description.
    </description>
    <author email="your.email@yourco.com" href="http://yourco.com">
        Your Team Name
    </author>
    <content src="mobileApps/mobileapp1/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*" />
    <allow-navigation href="https://*" />
    <allow-navigation href="data:*" />
    <preference name="InterceptRemoteRequests" value="all" />
    <preference name="NativeXHRLogging" value="full" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="StatusBarBackgroundColor" value="transparent" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="29" />
        <preference name="applicationId" value="com.oraclecorp.internal.ent1.vbcs" />
        <preference name="orientation" value="unspecified" />
        <preference name="CustomURLSchemePluginClearsAndroidIntent" value="true" />
        <icon background="res/android/icons/icon-mdpi_background.png" density="mdpi" foreground="res/android/icons/icon-mdpi_foreground.png" />
        <icon background="res/android/icons/icon-hdpi_background.png" density="hdpi" foreground="res/android/icons/icon-hdpi_foreground.png" />
        <icon background="res/android/icons/icon-xhdpi_background.png" density="xhdpi" foreground="res/android/icons/icon-xhdpi_foreground.png" />
        <icon background="res/android/icons/icon-xxhdpi_background.png" density="xxhdpi" foreground="res/android/icons/icon-xxhdpi_foreground.png" />
        <icon background="res/android/icons/icon-xxxhdpi_background.png" density="xxxhdpi" foreground="res/android/icons/icon-xxxhdpi_foreground.png" />
        <splash density="port-ldpi" src="res/android/splash/ldpi_p.9.png" />
        <splash density="land-ldpi" src="res/android/splash/ldpi_l.9.png" />
        <splash density="port-mdpi" src="res/android/splash/mdpi_p.9.png" />
        <splash density="land-mdpi" src="res/android/splash/mdpi_l.9.png" />
        <splash density="port-hdpi" src="res/android/splash/hdpi_p.9.png" />
        <splash density="land-hdpi" src="res/android/splash/hdpi_l.9.png" />
        <splash density="port-xhdpi" src="res/android/splash/xhdpi_p.9.png" />
        <splash density="land-xhdpi" src="res/android/splash/xhdpi_l.9.png" />
        <splash density="port-xxhdpi" src="res/android/splash/xxhdpi_p.9.png" />
        <splash density="land-xxhdpi" src="res/android/splash/xxhdpi_l.9.png" />
        <splash density="port-xxxhdpi" src="res/android/splash/xxxhdpi_p.9.png" />
        <splash density="land-xxxhdpi" src="res/android/splash/xxxhdpi_l.9.png" />
    </platform>
    <config-file parent="/*" target="AndroidManifest.xml">
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
        <uses-feature android:name="android.hardware.location.gps" />
    </config-file>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="target-device" value="universal" />
        <preference name="deployment-target" value="12.0" />
        <preference name="Orientation" value="all" />
        <preference name="WKWebViewOnly" value="true" />
        <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
        <feature name="CDVWKWebViewEngine">
            <param name="ios-package" value="CDVWKWebViewEngine" />
        </feature>
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>The camera may be used to take pictures that may be uploaded to a server.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSMicrophoneUsageDescription">
            <string>The microphone may be used to record voice that may be uploaded to a server.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>The photo library may be accessed to retrieve images that may be uploaded to a server.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>Your location may be determined for use within the app and may be attached to photos taken with the camera.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysUsageDescription">
            <string>Your location may be determined for use within the app and may be attached to photos taken with the camera.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string>Photos taken with the camera may be added to the photo library.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="LSApplicationQueriesSchemes">
            <array>
                <string>cydia</string>
            </array>
        </edit-config>
        <icon height="20" src="res/ios/icons/icon-20.png" width="20" />
        <icon height="40" src="res/ios/icons/icon-20@2x.png" width="40" />
        <icon height="60" src="res/ios/icons/icon-20@3x.png" width="60" />
        <icon height="29" src="res/ios/icons/icon-small.png" width="29" />
        <icon height="58" src="res/ios/icons/icon-small@2x.png" width="58" />
        <icon height="87" src="res/ios/icons/icon-small@3x.png" width="87" />
        <icon height="40" src="res/ios/icons/icon-small-40.png" width="40" />
        <icon height="80" src="res/ios/icons/icon-small-40@2x.png" width="80" />
        <icon height="120" src="res/ios/icons/icon-small-40@3x.png" width="120" />
        <icon height="50" src="res/ios/icons/icon-small-50.png" width="50" />
        <icon height="100" src="res/ios/icons/icon-small-50@2x.png" width="100" />
        <icon height="57" src="res/ios/icons/icon.png" width="57" />
        <icon height="114" src="res/ios/icons/icon@2x.png" width="114" />
        <icon height="60" src="res/ios/icons/icon-60.png" width="60" />
        <icon height="120" src="res/ios/icons/icon-60@2x.png" width="120" />
        <icon height="180" src="res/ios/icons/icon-60@3x.png" width="180" />
        <icon height="72" src="res/ios/icons/icon-72.png" width="72" />
        <icon height="144" src="res/ios/icons/icon-72@2x.png" width="144" />
        <icon height="76" src="res/ios/icons/icon-76.png" width="76" />
        <icon height="152" src="res/ios/icons/icon-76@2x.png" width="152" />
        <icon height="167" src="res/ios/icons/icon-83.5@2x.png" width="167" />
        <icon height="512" src="res/ios/icons/iTunesArtwork.png" width="512" />
        <icon height="1024" src="res/ios/icons/iTunesArtwork@2x.png" width="1024" />
        <splash src="res/ios/splash/Default@2x~iphone~anyany.png" />
        <splash src="res/ios/splash/Default@2x~iphone~comany.png" />
        <splash src="res/ios/splash/Default@2x~iphone~comcom.png" />
        <splash src="res/ios/splash/Default@3x~iphone~anyany.png" />
        <splash src="res/ios/splash/Default@3x~iphone~anycom.png" />
        <splash src="res/ios/splash/Default@3x~iphone~comany.png" />
        <splash src="res/ios/splash/Default@2x~ipad~anyany.png" />
        <splash src="res/ios/splash/Default@2x~ipad~comany.png" />
    </platform>
    <plugin name="cordova-plugin-file" spec="=6.0.2" />
    <plugin name="cordova-plugin-network-information" spec="=3.0.0" />
    <plugin name="cordova-plugin-geolocation" spec="=4.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="=6.0.0" />
    <plugin name="cordova-plugin-keyboard" spec="=1.2.0" />
    <plugin name="cordova-plugin-oracle-idm-auth" spec="=1.6.0" />
    <plugin name="cordova-plugin-camera" spec="=5.0.3">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="=2.0.3" />
    <plugin name="cordova-plugin-globalization" spec="=1.11.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="=5.0.0" />
    <plugin name="cordova-plugin-wkwebview-file-xhr" spec="=3.0.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="=5.0.2">
        <variable name="URL_SCHEME" value="oracle-vbcs" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="=1.3.5" />
    <plugin name="cordova-plugin-statusbar" spec="=2.4.3" />
    <plugin name="cordova-plugin-web-share" spec="=1.2.0" />
    <plugin name="cordova-plugin-file-opener2" spec="=3.0.5">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-iroot" spec="=3.1.0" />
    <engine name="android" spec="=9.1.0" />
    <engine name="ios" spec="=6.2.0" />
</widget>
